{
  "name": ":vendor/:package_name",
  "type": "library",
  "description": ":package_description",
  "keywords": [
    ":vendor",
    ":package_name"
  ],
  "homepage": "https://github.com/:vendor/:package_name",
  "license": "MIT",
  "authors": [
    {
      "name": ":author_name",
      "email": ":author_email",
      "homepage": ":author_website",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": ">=7.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      ":vendor\\:package_name\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      ":vendor\\:package_name\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs",
    "fix-style": "phpcbf"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
